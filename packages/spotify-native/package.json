{
  "name": "spotify-native",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prestorybook": "rnstl",
    "mkenv": "cd ../../scripts && sh env.sh ../packages/spotify-native/.env",
    "postinstall": "npm run mkenv",
    "storybook": "start-storybook -p 7007",
    "start-storybook-web": "./node_modules/@storybook/react/bin/index.js",
    "build-storybook-web": "./node_modules/@storybook/react/bin/build.js",
    "build-storybook": "build-storybook",
    "storybook-web": "yarn run start-storybook-web",
    "build:storybook-web": "yarn run build-storybook-web"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./app/components"
      ],
      "pattern": "**/*.stories.tsx",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.2",
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^6.0.0",
    "@react-native-picker/picker": "^1.16.3",
    "@react-navigation/native": "^5.7.3",
    "@react-navigation/stack": "^5.9.0",
    "@types/react-native-vector-icons": "^6.4.8",
    "dotenv": "^10.0.0",
    "inversify": "^5.0.5",
    "inversify-binding-decorators": "^4.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "0.64.0",
    "react-native-dotenv": "^2.5.5",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-reanimated": "^2.0.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^2.18.1",
    "react-native-vector-icons": "^8.1.0",
    "react-native-webview": "^11.3.2",
    "react-redux": "^7.2.2",
    "reflect-metadata": "^0.1.13",
    "spotify-core": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/runtime": "^7.14.8",
    "@react-native-community/cli-platform-android": "5.0.1-alpha.1",
    "@react-native-community/cli-platform-ios": "5.0.1-alpha.1",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/react": "^5.3.19",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@types/react-native": "^0.63.2",
    "@types/url-parse": "^1.4.3",
    "babel-jest": "^26.6.0",
    "babel-loader": "8.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "get-yarn-workspaces": "^1.0.2",
    "jest": "^25.1.0",
    "metro-config": "^0.56.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-native-dev-menu": "^4.0.2",
    "react-native-storybook-loader": "^2.0.4",
    "react-test-renderer": "16.13.1",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "workspaces": {
    "nohoist": [
      "react-native",
      "react-native/*",
      "react-native**",
      "@react-native-community/**",
      "@react-native-community",
      "@react-navigation/**",
      "redux",
      "react",
      "react-redux",
      "babel-jest",
      "babel-loader",
      "webpack",
      "webpack-dev-server",
      "babel-plugin-module-resolver",
      "@babel/*",
      "webpack",
      "webpack-cli",
      "webpack-dev-server"
    ]
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
